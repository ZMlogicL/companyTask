
mdf_user_custom
抄写代码路径：/cpu_tkernel/basefw/fj/file_mng/mdf/src
1、新建类文件mdfusercustom，并复制对应的.c , .h源码，修改makefile文件，并删除源文件mdf_user_custom.c，FS_SystemDependence.h

2、公有方法命名规范
VOID FS_ipcu_callback_dummy(UCHAR id, UINT32 pdata, UINT32 length, UINT32 cont, UINT32 totalLength) 
改为 VOID mdf_user_custom_ipcu_callback_dummy(MdfUserCustom* self, CallbackDummy callbackDummy);

FS_VOID FS_localtime(tm *time)  
改为 FS_VOID  mdf_user_custom_localtime(MdfUserCustom* self)

FS_VOID FS_SD_getTaskID(FS_DWORD *Taskid) 
改为 FS_VOID mdf_user_custom_sd_get_task_id(MdfUserCustom* self, FS_DWORD* Taskid);

FS_VOID *FS_SD_malloc(FS_DWORD size)  
改为 FS_VOID* mdf_user_custom_sd_malloc(MdfUserCustom* self, FS_DWORD size);

FS_VOID FS_SD_free(FS_VOID *address)  
改为 FS_VOID mdf_user_custom_sd_free(MdfUserCustom* self, FS_VOID* address);

void *FS_memcpy(void *buf1, const void *buf2, SIZE_T n) 
改为 kpointer mdf_user_custom_memcpy(MdfUserCustom* self, kpointer buf1, kconstpointer buf2, SIZE_T n);







vfs
抄写代码路径：/cpu_tkernel/basefw/fj/file_mng/vfs/src

1、建立所抄代码类并复制对应.c及.h代码，删除原.c及.h文件（vfs.c , vfs.h），修改makefile文件；
2、拆分文件为：vfsone、vfstwo、vfsthree、vfstool四个类

3、公有方法命名规范：
extern FW_INT32 BF_Vfs_Open(const FW_CHAR* fname, FW_USHORT mode, FW_USHORT amode);  
修改为  FW_INT32 vfs_two_open(VfsTwo* self, const FW_CHAR* fname, FW_USHORT mode, FW_USHORT amode);

extern FW_INT32 BF_Vfs_Close(FW_INT32 fileNo);  
修改为  FW_INT32   vfs_two_close(VfsTwo* self, FW_INT32 fileNo);

extern FW_INT32 BF_Vfs_Read(FW_INT32 fileNo, FW_VOID* addr, FW_ULONG count, FW_ULONG* length);  
修改为  FW_INT32 vfs_two_read(VfsTwo* self, FW_INT32 fileNo, const FW_VOID* addr, FW_ULONG count, FW_ULONG* length);

extern FW_INT32 BF_Vfs_Write(FW_INT32 fileNo, const  FW_VOID* addr, FW_ULONG count, FW_ULONG* length);  
修改为  FW_INT32 vfs_two_write(VfsTwo* self, FW_INT32 fileNo, const  FW_VOID* addr, FW_ULONG count, FW_ULONG* length);

extern FW_INT32 BF_Vfs_Lseek(FW_INT32 fileNo, FW_INT32 offset, FW_USHORT whence, FW_ULONG* length);  
修改为  FW_INT32 vfs_two_lseek(VfsTwo* self, FW_INT32 fileNo, FW_INT32 offset, FW_USHORT whence, FW_ULONG* length);

extern FW_INT32 BF_Vfs_Lseek64(FW_INT32 fileNo, FW_INT64 offset, FW_USHORT whence, FW_INT64* length);  
修改为  FW_INT32 vfs_two_lseek64(VfsTwo* self, FW_INT32 fileNo, FW_INT64 offset, FW_USHORT whence, FW_INT64* length);

extern FW_INT32 BF_Vfs_Start_MDF( FW_VOID );  
修改为  FW_INT32 vfs_two_start_mdf(VfsTwo* self);

extern FW_INT32 BF_Vfs_Init_Drv( FW_CHAR* drvname, FW_INT32 mediaid );  
修改为  FW_INT32 vfs_two_init_drv(VfsTwo* self, FW_CHAR* drvname, FW_INT32 mediaid);

extern FW_INT32 BF_Vfs_End_Drv( FW_CHAR* drvname );  
修改为  FW_INT32 vfs_two_end_drv(VfsTwo* self, FW_CHAR* drvname);

extern FW_INT32 BF_Vfs_Set_Write_Err_Cb( FW_UINT32 fNo, VFS_SET_WRITE_ERR_CB pCallback );  
修改为  FW_INT32 vfs_two_set_write_err_cb(VfsTwo* self, FW_UINT32 fNo, VFS_SET_WRITE_ERR_CB pCallback);

extern FW_INT32 BF_Vfs_Stat_VFS( FW_CHAR* drvname, T_BF_FS_IF_STATVFS* buf ); 
修改为  FW_INT32 vfs_one_stat_vfs(VfsOne* self, const FW_CHAR* drvname, FsIfStatVfs* buf);

extern FW_INT32 BF_Vfs_Get_Free_Space( FW_CHAR* drvname, FW_UINT32* blksize, FW_UINT32* blocks, FW_UINT32* bfree );  
修改为 FW_INT32 vfs_one_get_free_space(VfsOne* self, const FW_CHAR* drvname,FW_UINT32* blksize, FW_UINT32* blocks, FW_UINT32* bfree);

extern FW_INT32 BF_Vfs_Get_Cur_Drv( FW_CHAR* drvname ); 
 修改为  FW_INT32 vfs_one_get_cur_drv(VfsOne* self, FW_CHAR* drvname);
 
extern FW_INT32 BF_Vfs_Set_Vol_Label( FW_CHAR* drvname, FW_CHAR* label );  
修改为  FW_INT32 vfs_one_set_vol_label(VfsOne* self, const FW_CHAR* drvname, const FW_CHAR* label);

extern FW_INT32 BF_Vfs_Get_Vol_Label( FW_CHAR* drvname, FW_CHAR* label );  
修改为  FW_INT32 vfs_one_get_vol_label(VfsOne* self, FW_CHAR* drvname, FW_CHAR* label);

extern FW_INT32 BF_Vfs_Fsync( FW_INT32 fileNo );  
修改为  FW_INT32 vfs_one_fsync(VfsOne* self, FW_INT32 fileNo);

extern FW_INT32 BF_Vfs_Sync( FW_CHAR* drvname );  
修改为  FW_INT32 vfs_one_sync(VfsOne* self, const FW_CHAR* drvname);

extern FW_INT32 BF_Vfs_Set_Protection_Mode( FW_CHAR* drvname, FW_UINT32 mode );  
修改为  FW_INT32 vfs_one_set_protection_mode(VfsOne* self, const FW_CHAR* drvname, FW_UINT32 mode);

extern FW_INT32 BF_Vfs_Rename( FW_CHAR* srcname, FW_CHAR* dstname );  
修改为  FW_INT32 vfs_one_rename(VfsOne* self, const FW_CHAR* srcname, const FW_CHAR* dstname);

extern FW_INT32 BF_Vfs_Stat( FW_CHAR* fname, T_BF_FS_IF_STAT* sbuf );  
修改为  FW_INT32 vfs_one_stat(VfsOne* self, const FW_CHAR* fname, FsIfStat* sbuf);

extern FW_INT32 BF_Vfs_Set_Attribute( FW_CHAR* fname, FW_INT32 attributes ); 
修改为  FW_INT32 vfs_one_set_attribute(VfsOne* self, const FW_CHAR* fname, FW_INT32 attributes);

