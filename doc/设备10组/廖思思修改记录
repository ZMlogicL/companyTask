 修改为  FW_INT32 vfs_one_set_attribute_multi(VfsOne* self, const FW_CHAR* filenameList[][VfsOne_SETATTRIBUTE_MULTI_SHORTNAME_MAX], const FW_UCHAR* attributes, const FW_UCHAR* attrMode, const FW_INT32* fnum);

extern FW_INT32 BF_Vfs_Get_Attribute( FW_CHAR* fname, FW_UCHAR* attributes );  
修改为  FW_INT32 vfs_one_get_attribute(VfsOne* self, const FW_CHAR* fname, FW_UCHAR* attributes);

extern FW_INT32 BF_Vfs_Utime( FW_CHAR* fname, T_BF_FS_utimbuf* times );  
修改为  FW_INT32 vfs_one_utime(VfsOne* self, const FW_CHAR* fname, const FsUtimbuf* times);

extern FW_INT32 BF_Vfs_Utime_Ex( FW_CHAR* fname, FW_UINT32 utime_mode, FsUtimbufEx* times );  
修改为  FW_INT32 vfs_one_utime_ex(VfsOne* self, const FW_CHAR* fname, const FW_UINT32 utimeMode, const FsUtimbufEx* times);

extern FW_INT32 BF_Vfs_Create(FW_CHAR* dirname, FW_INT32 amode);
修改为  FW_INT32   vfs_one_create(VfsOne* self, const FW_CHAR* dirname, const FW_INT32 amode);

extern FW_INT32 BF_Vfs_Copy(FW_CHAR* srcname, FW_CHAR* dstname);
