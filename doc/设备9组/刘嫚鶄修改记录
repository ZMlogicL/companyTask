#define FjEncoder_MOVIE_VIDEO_LEVEL_5_2			52	/**< LEVEL : "5.2" */
#define FjEncoder_MOVIE_VIDEO_LEVEL_6				60	/**< LEVEL : "6" */
#define FjEncoder_MOVIE_VIDEO_LEVEL_6_1			61	/**< LEVEL : "6.1" */
